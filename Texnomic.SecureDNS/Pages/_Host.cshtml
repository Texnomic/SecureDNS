@page "/"
@namespace Texnomic.SecureDNS.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>SecureDNS</title>
    <base href="~/" />


    <!-- BootStrap -->
    <link href="/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <script src="/lib/bootstrap/dist/js/bootstrap.min.js"></script>

    <!-- Blazor Custom CSS -->
    <link href="css/site.css" rel="stylesheet" />

    <!-- Font Awesome -->
    <link href="/lib/font-awesome/css/all.min.css" rel="stylesheet" />
    <script src="/lib/font-awesome/js/all.min.js"></script>

    <!-- jQuery -->
    <script src="/lib/jquery/jquery.min.js"></script>

    <!-- Popper -->
    <script src="/lib/popper.js/popper.min.js"></script>

    <!-- Blazorise -->
    <link href="/lib/blazorise/blazorise.css" rel="stylesheet" />
    <link href="/lib/blazorise/blazorise.bootstrap.css" rel="stylesheet" />
    <link href="/lib/blazorise/blazorise.sidebar.css" rel="stylesheet" />
    <link href="/lib/blazorise/blazorise.snackbar.css" rel="stylesheet" />
    <script src="/lib/blazorise/blazorise.js"></script>
    <script src="/lib/blazorise/blazorise.bootstrap.js"></script>
    <script src="/lib/blazorise/blazorise.charts.js"></script>

